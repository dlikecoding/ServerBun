{
  "name": "photos",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run server/index.ts",
    "dev": "bun run --watch server/index.ts",
    "startdb": "brew services start mysql",
    "stopdb": "brew services stop mysql",
    "buildS": "bun build server/index.ts --outdir=../serverBunX --target=bun && cp -r ./server/dbms ./server/ai_models ../serverBunX",
    "format": "bun prettier . --write"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "3.4.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.2",
    "@simplewebauthn/server": "^13.1.1",
    "hono": "^4.6.14",
    "mysql2": "^3.12.0",
    "zod": "^3.24.1"
  }
}
